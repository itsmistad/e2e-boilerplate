{
    "name": "@apps/universal-ui",
    "main": "expo-router/entry",
    "version": "1.0.0",
    "expo": {
        "doctor": {
            "reactNativeDirectoryCheck": {
                "exclude": [
                    "tamagui",
                    "@apps/orchestrator",
                    "@tamagui/config",
                    "@trpc/client",
                    "@trpc/react-query",
                    "jotai",
                    "superjson"
                ]
            }
        }
    },
    "scripts": {
        "clean:jest": "jest --clearCache",
        "clean:xcode": "chmod +x ./scripts/clean-xcode.sh && ./scripts/clean-xcode.sh",
        "clean:workspace": "chmod +x ./scripts/clean-workspace.sh && ./scripts/clean-workspace.sh",
        "clean": "pnpm clean:jest && pnpm clean:xcode && pnpm clean:workspace",
        "start:metro": "expo start -c",
        "start:android": "expo run:android",
        "start:ios": "expo run:ios",
        "start:web": "expo run:web",
        "prebuild:ios": "expo prebuild --clean --platform ios",
        "prebuild:android": "expo prebuild --clean --platform android",
        "build": "pnpm run build:ios && pnpm run build:android",
        "build:ios": "expo run:ios --configuration Release",
        "build:android": "expo run:android --variant release",
        "test": "jest",
        "test:watch": "jest --watchAll",
        "lint": "eslint",
        "lint:fix": "eslint --fix",
        "preinstall": "npx only-allow pnpm",
        "android": "expo run:android",
        "ios": "expo run:ios",
        "tsc": "tsc --noEmit"
    },
    "dependencies": {
        "@apps/orchestrator": "workspace:*",
        "@expo/vector-icons": "^14.0.2",
        "@gorhom/bottom-sheet": "^5.2.6",
        "@gorhom/portal": "^1.0.14",
        "@react-navigation/native": "^7.0.15",
        "@tamagui/babel-plugin": "^1.125.23",
        "@tamagui/config": "^1.125.23",
        "@tamagui/metro-plugin": "^1.125.23",
        "@tamagui/web": "^1.125.23",
        "@tanstack/react-query": "^5.62.10",
        "@trpc/client": "11.0.0-rc.666",
        "@trpc/react-query": "11.0.0-rc.666",
        "date-fns": "^4.1.0",
        "expo": "~52.0.37",
        "expo-blur": "~14.0.3",
        "expo-constants": "~17.0.7",
        "expo-dev-client": "^5.0.12",
        "expo-font": "~13.0.4",
        "expo-haptics": "~14.0.1",
        "expo-linking": "~7.0.5",
        "expo-router": "~4.0.17",
        "expo-splash-screen": "~0.29.22",
        "expo-status-bar": "~2.0.1",
        "expo-symbols": "~0.2.2",
        "expo-system-ui": "~4.0.8",
        "expo-web-browser": "~14.0.2",
        "jotai": "^2.11.0",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "react-native": "0.76.7",
        "react-native-gesture-handler": "~2.20.2",
        "react-native-reanimated": "~3.16.1",
        "react-native-safe-area-context": "4.12.0",
        "react-native-screens": "~4.4.0",
        "react-native-web": "~0.19.13",
        "react-native-webview": "13.12.5",
        "superjson": "^2.2.2",
        "tamagui": "^1.125.23",
        "zod": "^3.24.1"
    },
    "devDependencies": {
        "@babel/core": "^7.25.2",
        "@babel/preset-env": "^7.26.0",
        "@babel/preset-flow": "^7.27.1",
        "@babel/preset-typescript": "^7.27.1",
        "@babel/runtime": "^7.26.0",
        "@eslint/compat": "^1.2.5",
        "@eslint/js": "^9.36.0",
        "@eslint/markdown": "^6.2.2",
        "@jest/globals": "^30.1.2",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/jest-dom": "^6.8.0",
        "@testing-library/react": "^16.3.0",
        "@testing-library/react-native": "^13.3.3",
        "@testing-library/user-event": "^14.6.1",
        "@types/jest": "^29.5.12",
        "@types/node": "^22.10.2",
        "@types/react": "~18.3.12",
        "@types/react-dom": "^19.1.9",
        "@types/react-test-renderer": "^18.3.0",
        "@typescript-eslint/parser": "^8.44.0",
        "babel-plugin-module-resolver": "^5.0.2",
        "babel-preset-expo": "^54.0.2",
        "eslint": "^9.36.0",
        "eslint-config-expo": "~8.0.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-typescript": "^3.7.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-jest": "^29.0.1",
        "eslint-plugin-prettier": "^5.2.3",
        "eslint-plugin-react": "^7.37.3",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-native": "^5.0.0",
        "eslint-plugin-yml": "^1.16.0",
        "jest": "^30.1.3",
        "jest-expo": "~52.0.5",
        "jsdom": "^27.0.0",
        "knip": "^5.63.1",
        "msw": "^2.11.3",
        "prettier": "^3.4.2",
        "react-test-renderer": "18.3.1",
        "typescript": "5.7.3",
        "typescript-eslint": "8.44.0"
    },
    "private": true
}
